{
  "name": "@bns-x/api-types",
  "version": "0.0.1",
  "description": "",
  "files": [
    "dist"
  ],
  "private": true,
  "scripts": {
    "r": "NODE_ENV=development tsx -r dotenv-flow/config",
    "rw": "NODE_ENV=development tsx watch -r dotenv-flow/config",
    "rp": "NODE_ENV=production tsx -r dotenv-flow/config",
    "rpw": "NODE_ENV=production tsx watch -r dotenv-flow/config",
    "dev:mainnet": "pnpm rpw scripts/server.ts",
    "dev:testnet": "NODE_ENV=staging pnpm tsx watch -r dotenv-flow/config scripts/server.ts",
    "dev": "pnpm rw scripts/server.ts",
    "start": "tsx scripts/server.ts",
    "test:migrate": "dotenv -e .env.test -- prisma migrate dev --name test-init",
    "test:server": "dotenv -e .env.test -- tsx scripts/server.ts",
    "test:run": "pnpm test:migrate && pnpm test:server",
    "sync": "pnpm r scripts/sync-prints.ts",
    "db:reset": "prisma migrate reset",
    "typecheck": "tsc --noEmit",
    "prebuild": "shx rm -rf dist",
    "build": "tsup",
    "build:openapi": "pnpm r scripts/openapi-generator.ts"
  },
  "author": "Hank Stoever",
  "license": "ISC",
  "dependencies": {
    "@bns-x/core": "workspace:*",
